  "problem_patterns": {
    "typical_distribution": {
      "i1": "Usually beginner - basic pattern recognition",
      "i2": "Usually intermediate - morphological paradigms",
      "i3": "Usually intermediate - writing systems or syntax",
      "i4": "Usually advanced - complex morphology or historical",
      "i5": "Usually advanced/expert - computational or complex semantics"
    },
    "common_topics": {
      "morphology": "Most common - noun classes, verb conjugation, case systems",
      "writing_systems": "Very common - scripts, syllabaries, decipherment",
      "phonology": "Common - sound patterns, tone, syllable structure",
      "syntax": "Moderate - word order, agreement, case marking",
      "semantics": "Less common but important - kinship, color terms",
      "computational": "Increasing - formal patterns, algorithms",
      "sign_language": "Rare but notable - ASL, gesture systems"
    },
    "language_diversity": {
      "frequent_families": [
        "caucasian (Budukh, Lak, Georgian, Lezgian)",
        "bantu (Swahili, Zulu, Kinyarwanda)",
        "australian (Warlpiri, Kaytetye, Murrinh-Patha)",
        "uto-aztecan (Nahuatl, Hopi)"
      ],
      "underrepresented": [
        "sino-tibetan",
        "austroasiatic",
        "tai-kadai"
      ]
    }
  },
